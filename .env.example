# E2B API Key - get from https://e2b.dev/dashboard
E2B_API_KEY=e2b_xxx

# Anthropic API Key - for Claude Agent SDK
ANTHROPIC_API_KEY=sk-ant-xxx

# Claude model for Agent SDK (optional, defaults to claude-sonnet-4-5)
# Options: claude-opus-4-1-20250805, claude-sonnet-4-5-20250929, claude-haiku-4-5-20251001
CLAUDE_MODEL=claude-opus-4-5-20251101

# Sandbox mode: 'local' for development, 'e2b' for production
SANDBOX_MODE=local

# Keboola configuration (for data apps)
WORKSPACE_ID=xxx
BRANCH_ID=xxx
KBC_URL=https://connection.keboola.com/
KBC_TOKEN=xxx
